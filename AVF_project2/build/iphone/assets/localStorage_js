var ui=require("ui");Ti.Database.install("weather.sqlite","weather"),Ti.App.Properties.setString("weatherInfo",JSON.stringify(weatherInfo));var weatherInfo=JSON.parse(Ti.App.Properties.getString("weatherInfo"));exports.create=function(e){var a=Ti.Database.open("weather");a.execute("CREATE TABLE IF NOT EXISTS weatherInfo (id INTEGER PRIMARY KEY, temp TEXT,  location TEXT, wind TEXT, iconWord TEXT, iconPic TEXT, time1 TEXT, time2 TEXT, windDir TEXT, feels TEXT, chill TEXT, dayName TEXT, dayName2 TEXT, dayName3 TEXT, hi TEXT, lo TEXT, day1 TEXT, month1 TEXT, date1 TEXT, hi1 TEXT, lo1 TEXT, day2 TEXT, month2 TEXT, date2 TEXT, hi2 TEXT, lo2 TEXT, day3 TEXT, month3 TEXT, date3 TEXT, hi3 TEXT, lo3 TEXT, day4 TEXT, month4 TEXT, date4 TEXT, hi4 TEXT, lo4 TEXT, day5 TEXT, month5 TEXT, date5 TEXT, hi5 TEXT, lo5 TEXT)"),a.execute("INSERT INTO weatherInfo (temp, location, wind, iconWord, iconPic, time1, time2, windDir, feels, chill, dayName, dayName2, dayName3, hi, lo, day1, month1, date1, hi1, lo1, day2, month2, date2, hi2, lo2, day3, month3, date3, hi3, lo3, day4, month4, date4, hi4, lo4, day5, month5, date5, hi5, lo5) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ?, ? )",e.temp,e.location,e.wind,e.iconWord,e.iconPic,e.time1,e.time2,e.windDir,e.feels,e.chill,e.dayName,e.dayName2,e.dayName3,e.hi,e.lo,e.day1,e.month1,e.date1,e.hi1,e.lo1,e.day2,e.month2,e.date2,e.hi2,e.lo2,e.day3,e.month3,e.date3,e.hi3,e.lo3,e.day4,e.month4,e.date4,e.hi4,e.lo4,e.day5,e.month5,e.date5,e.hi5,e.lo5),a.close(),read()};var read=function(){var e=Ti.Database.open("weather");e.execute("CREATE TABLE IF NOT EXISTS weatherInfo (id INTEGER PRIMARY KEY, temp TEXT, location TEXT, wind TEXT, iconWord TEXT, iconPic TEXT, time1 TEXT, time2 TEXT, windDir TEXT, feels TEXT, chill TEXT, dayName TEXT, dayName2 TEXT, dayName3 TEXT, hi TEXT, lo TEXT, day1 TEXT, month1 TEXT, date1 TEXT, hi1 TEXT, lo1 TEXT, day2 TEXT, month2 TEXT, date2 TEXT, hi2 TEXT, lo2 TEXT, day3 TEXT, month3 TEXT, date3 TEXT, hi3 TEXT, lo3 TEXT, day4 TEXT, month4 TEXT, date4 TEXT, hi4 TEXT, lo4 TEXT, day5 TEXT, month5 TEXT, date5 TEXT, hi5 TEXT, lo5 TEXT)");for(var a=e.execute("SELECT temp, location, wind, iconWord, iconPic, time1, time2, windDir, feels, chill, dayName, dayName2, dayName3, hi, lo, day1, month1, date1, hi1, lo1, day2, month2, date2, hi2, lo2, day3, month3, date3, hi3, lo3, day4, month4, date4, hi4, lo4, day5, month5, date5, hi5, lo5 FROM weatherInfo");a.isValidRow();){var t={temp:a.fieldByName("temp"),location:a.fieldByName("location"),wind:a.fieldByName("wind"),iconWord:a.fieldByName("iconWord"),iconPic:a.fieldByName("iconPic"),time1:a.fieldByName("time1"),time2:a.fieldByName("time2"),windDir:a.fieldByName("windDir"),feels:a.fieldByName("feels"),chill:a.fieldByName("chill"),dayName:a.fieldByName("dayName"),dayName2:a.fieldByName("dayName2"),dayName3:a.fieldByName("dayName3"),hi:a.fieldByName("hi"),lo:a.fieldByName("lo"),day1:a.fieldByName("day1"),month1:a.fieldByName("month1"),date1:a.fieldByName("date1"),hi1:a.fieldByName("hi1"),lo1:a.fieldByName("lo1"),day2:a.fieldByName("day2"),month2:a.fieldByName("month2"),date2:a.fieldByName("date2"),hi2:a.fieldByName("hi2"),lo2:a.fieldByName("lo2"),day3:a.fieldByName("day3"),month3:a.fieldByName("month3"),date3:a.fieldByName("date3"),hi3:a.fieldByName("hi3"),lo3:a.fieldByName("lo3"),day4:a.fieldByName("day4"),month4:a.fieldByName("month4"),date4:a.fieldByName("date4"),hi4:a.fieldByName("hi4"),lo4:a.fieldByName("lo4"),day5:a.fieldByName("day5"),month5:a.fieldByName("month5"),date5:a.fieldByName("date5"),hi5:a.fieldByName("hi5"),lo5:a.fieldByName("lo5")};a.next()}a.close(),e.close(),ui.buildUi(t)};